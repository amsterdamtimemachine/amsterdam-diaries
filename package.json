{
  "name": "amsterdam-diaries",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "git diff --name-only --diff-filter=MA main | grep -E '\\.ts$|\\.vue$' | xargs eslint",
    "lint:all": "eslint './**/*.{vue,ts}'",
    "lint:fix": "git diff --name-only --diff-filter=MA main | grep -E '\\.ts$|\\.vue$' | xargs eslint --fix",
    "lint:fix:all": "eslint --fix './**/*.{vue,ts}",
    "test": "vitest",
    "ts:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.5.1",
    "jsonld": "^8.3.2",
    "leaflet": "^1.9.4",
    "nuxt": "^3.11.1",
    "pinia": "^2.1.7",
    "ress": "^5.0.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@types/geojson": "^7946.0.14",
    "@types/jsonld": "^1.5.13",
    "@types/leaflet": "^1.9.11",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
